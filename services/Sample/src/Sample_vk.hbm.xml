<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mymobileapp.sample">
    <class entity-name="com.mymobileapp.sample.Table1" table="TABLE1" name="com.mymobileapp.sample.Table1">
        <id name="id" type="integer">
            <column name="ID" length="255" not-null="true" precision="19"/>
            <generator class="identity"/>
        </id>
        <property name="empId" type="integer">
            <column name="EmpId" length="255" not-null="false" precision="19"/>
        </property>
        <property name="role" type="string">
            <column name="Role" length="255" not-null="false" precision="19"/>
        </property>
        <property name="doj" type="date">
            <column name="DOJ" length="10" not-null="false" precision="19"/>
        </property>
    </class>
</hibernate-mapping>